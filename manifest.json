{
  "name": "Tailwind UI -> JSX",
  "version": "1.3",
  "description": "Automatic translation of Tailwind UI components to JSX-compatible components.",
  "permissions": ["https://tailwindui.com/*"],
  "content_scripts": [
    {
      "matches": ["https://tailwindui.com/*"],
      "js": ["lib/prism.min.js", "lib/prism-jsx.min.js", "main.js"]
    }
  ],
  "icons": {
    "128": "icons/icon.png"
  },
  "manifest_version": 2
}
